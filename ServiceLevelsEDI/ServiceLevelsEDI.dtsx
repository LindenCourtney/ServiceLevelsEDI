        /// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
        {
            Dts.Variables["User::TodaysDate"].Value = DateTime.Now.ToLongDateString();
            Dts.Variables["User::GetEmailRecipients"].Value = GetAllEmailRecipients();
            Dts.Variables["User::AllDailyServiceLevelsFileLocation"].Value = FindAndRenameFile("All Daily Service Levels");
            Dts.Variables["User::DailyRetailServiceLevelsFileLocation"].Value = FindAndRenameFile("Daily Retail Service Levels");
            Dts.Variables["User::DailyB2BServiceLevelsFileLocation"].Value = FindAndRenameFile("Daily B2B Service Levels");


            Dts.TaskResult = (int)ScriptResults.Success;
        }

        private string FindAndRenameFile(string serviceLevelReportName)
        {
            //find file
            string serviceLevelReportPath = $@"\\lf-public\Granville-Public\Retail Shipping\Service level report\Service Level Reports EDI\{serviceLevelReportName}";
            var files = Directory.GetFiles(serviceLevelReportPath, $"{serviceLevelReportName}_{DateTime.Now:yyyy_MM_dd}*");

            //check if file exists or if there is more than one file
            if (files.Length == 0)
            {
                Dts.Variables["User::Error"].Value = $"Error: {serviceLevelReportName} report for {DateTime.Now:dd/MM/yyyy} is not in directory {serviceLevelReportPath}. File could not be emailed out.";
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
            else if (files.Length > 1)
            {
                Dts.Variables["User::Warning"].Value = $"Warning: More than 1 {serviceLevelReportName} report for {DateTime.Now:dd/MM/yyyy} is in directory {serviceLevelReportPath}, using only the first file.";
            }

            return files[0];
        }

        public string GetAllEmailRecipients()
        {
            return 
                "david.beatty@lindenfoods.com;" +
                "declan.bogue@lindenfoods.com;" +
                "elainew@lindenfoods.com;" +
                "Jacqueline.mcnally@lindenfoods.com;" +
                "joanne.quinn@lindenfoods.com;" +
                "mark.ferrier@lindenfoods.com;" +
                "pauricg@lindenfoods.com;" +
                "retailshipping@lindenfoods.com;" +
                "shauns@lindenfoods.com;" +
                "victoria.millar@lindenfoods.com;" +
                "vitoc@lindenfoods.com";
        }

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
